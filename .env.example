# Server Configuration
PORT=5000
NODE_ENV=development

# Authentication
JWT_SECRET=your_super_secret_jwt_key_change_in_production

# Database Configuration (for future use)
# DB_HOST=localhost
# DB_PORT=5432
# DB_USER=mediconnect_user
# DB_PASSWORD=your_secure_password
# DB_NAME=mediconnect_db

# Email Configuration (for future use)
# EMAIL_HOST=smtp.example.com
# EMAIL_PORT=587
# EMAIL_USER=notifications@mediconnect.com
# EMAIL_PASSWORD=your_email_password

# File Storage (for future use)
# S3_BUCKET=mediconnect-uploads
# S3_ACCESS_KEY=your_s3_access_key
# S3_SECRET_KEY=your_s3_secret_key
# S3_REGION=us-east-1

# API Keys (for future use)
# STRIPE_API_KEY=your_stripe_api_key
# GOOGLE_MAPS_API_KEY=your_google_maps_api_key
# TWILIO_ACCOUNT_SID=your_twilio_account_sid
# TWILIO_AUTH_TOKEN=your_twilio_auth_token

# Monitoring and Analytics (for future use)
# SENTRY_DSN=your_sentry_dsn
# GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Feature Flags
ENABLE_VIDEO_APPOINTMENTS=true
ENABLE_BLOG_FUNCTIONALITY=true
ENABLE_FORUM_FUNCTIONALITY=true
ENABLE_VIDEO_REELS=true

# Note: Rename this file to .env and add appropriate values for your environment
# Do not commit the actual .env file to version control